"""
ADVENT OF CODE DAY 3 PROBLEM 1
"""
goodTri = 0

def isTriangle(a, b, c):
  if a + b > c and a + c > b and b + c > a:
    return True
  else:
    return False

with open("file1.txt") as f:
    for line in f:
        l = line
        l = l.split( )
        if isTriangle(int(l[0]), int(l[1]), int(l[2])):
          goodTri += 1
        
print goodTri

