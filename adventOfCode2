instructions = ["",
"",
"",
"",
""]


#instructions = ["ULL", "RRDDD", "LURDL", "UUUUD"]
"""
ADVENT OF CODE DAY 2 PROBLEM 2
"""
pos = [2, 0]

#keypad = [[1, 2, 3], [4, 5, 6], [7, 8, 9]]
keypad = [["Z", "Z", 1, "Z", "Z"], ["Z", 2, 3, 4, "Z"], [5, 6, 7, 8, 9], ["Z", "A", "B", "C", "Z"], ["Z", "Z", "D", "Z", "Z"]]

moves = {}

code = []

for i in instructions:
  for j in range(0, len(i)):
    if i[j] == "R" and pos[1] != 4:
      k = keypad[pos[0]][pos[1] + 1]
      if k != "Z":
        pos[1] += 1
    elif i[j] == "L" and pos[1] != 0:
      k = keypad[pos[0]][pos[1] - 1]
      if k != "Z":
        pos[1] -= 1
    elif i[j] == "U" and pos[0] != 0:
      k = keypad[pos[0] - 1][pos[1]]
      if k != "Z":
        pos[0] -= 1
    elif i[j] == "D" and pos[0] != 4:
      k = keypad[pos[0] + 1][pos[1]]
      if k != "Z":
        pos[0] += 1
  code.append(keypad[pos[0]][pos[1]])
  
print code
